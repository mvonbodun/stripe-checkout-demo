{
  "functions": {
    "app/api/catalog-categories/route.js": {
      "includeFiles": [
        "proto/**",
        "app/lib/proto-descriptors/*.json"
      ]
    },
    "app/api/products/slug/[slug]/route.js": {
      "includeFiles": [
        "proto/**",
        "app/lib/proto-descriptors/*.json"
      ]
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "buildCommand": "npm run build"
}
